<?xml version="1.0"?>
<Container version="2">
  <Name>check-qb</Name>
  <Repository>unraiders/check-qb</Repository>
  <Registry>https://hub.docker.com/r/unraiders/check-qb/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/unraiders/check-qb</Support>
  <Project>https://github.com/unraiders/check-qb</Project>
  <Overview>Monitoreo de QBittorrent con notificaciones de Telegram</Overview>
  <Category>Tools: Status:Stable</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/unraiders/check-qb/master/check-qb-icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1702722897</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Config Name="QB_IP" Target="QB_IP" Default="" Mode="" Description="IP del cliente qBittorrent" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="QB_PORT" Target="QB_PORT" Default="" Mode="" Description="Puerto del cliente qBittorrent" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="QB_USER" Target="QB_USER" Default="" Mode="" Description="Usuario del cliente qBittorrent" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="QB_PASSWORD" Target="QB_PASSWORD" Default="" Mode="" Description="Password del cliente qBittorrent" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="TELEGRAM_BOT_TOKEN" Target="TELEGRAM_BOT_TOKEN" Default="" Mode="" Description="Telegram Bot Token" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="TELEGRAM_CHAT_ID" Target="TELEGRAM_CHAT_ID" Default="" Mode="" Description="Telegram Chat ID" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="PAUSADO" Target="PAUSADO" Default="1" Mode="" Description="Umbral de número de torrents en pausa" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="NO_TRACKER" Target="NO_TRACKER" Default="1" Mode="" Description="Umbral de número de torrents sin tracker" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="NOMBRE" Target="NOMBRE" Default="1" Mode="" Description="Mostrar nombres de torrents en Telegram. (0 = No / 1 = Si)" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="RESUMEN" Target="RESUMEN" Default="1" Mode="" Description="Mostrar resumen del estado de los torrents en Telegram. (0 = No / 1 = Si)" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="HORA" Target="HORA" Default="7:00" Mode="" Description="Hora de ejecutar el script (formato de 24 horas, p. ej., 7:00)" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="TZ" Target="TZ" Default="Europe/Madrid" Mode="" Description="Zona horaria" Type="Variable" Display="always" Required="true" Mask="false"/>
</Container>
